---
import faviconICO from '../icons/favicon/favicon.ico'
import favicon96 from '../icons/favicon/favicon-96x96.png'
import faviconSVG from '../icons/favicon/favicon.svg'
import faviconAPPLE from '../icons/favicon/apple-touch-icon.png'

---


<link rel="icon" type="image/x-icon" href={faviconICO}>
<link rel="icon" type="image/png" href={favicon96.src} sizes="96x96" />
<link rel="icon" type="image/svg+xml" href={faviconSVG.src} />
<link rel="apple-touch-icon" sizes="180x180" href={faviconAPPLE.src} />
<meta name="apple-mobile-web-app-title" content="Portfolio" />
